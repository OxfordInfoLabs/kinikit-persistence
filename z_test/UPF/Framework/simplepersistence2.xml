<UPF>

	<ObjectMappers>

		<Object className="Kinikit\Persistence\UPF\Framework\Contact" location="businessobjects">
			<Fields>
				<Field fieldName="name" required="true" />
				<Field fieldName="telephone" primaryKey="true" required="true" />
			</Fields>

			<Interceptors>
				<TestObjectInterceptor1 phpNameSpace="Kinikit\Persistence\UPF\Framework" />
				<TestObjectInterceptor2 phpNameSpace="Kinikit\Persistence\UPF\Framework"/>
			</Interceptors>

		</Object>


		<Object className="Kinikit\Persistence\UPF\Framework\ContactVariant">
			<Fields>
				<Field fieldName="name" required="true" />
				<Field fieldName="telephone" primaryKey="true" required="true" />


			</Fields>

			<Interceptors>
				<TestObjectInterceptor1 phpNameSpace="Kinikit\Persistence\UPF\Framework"/>
				<TestObjectInterceptor2 phpNameSpace="Kinikit\Persistence\UPF\Framework"/>
			</Interceptors>

		</Object>

		<Object className="Kinikit\Persistence\UPF\Framework\Address">
			<Fields>
				<Field fieldName="streetAddress" required="true" />
				<Field fieldName="city" primaryKey="true" />
			</Fields>
		</Object>

	</ObjectMappers>



</UPF>